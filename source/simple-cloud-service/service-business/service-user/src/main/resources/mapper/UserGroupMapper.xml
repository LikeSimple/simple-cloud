<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.newtouch.cloud.service.business.user.mapper.UserGroupMapper">
  <resultMap id="BaseResultMap" type="com.newtouch.cloud.service.business.user.model.UserGroup">
    <id column="user_id" jdbcType="CHAR" property="userId" />
    <id column="group_id" jdbcType="CHAR" property="groupId" />
  </resultMap>
  <insert id="insertSelective" parameterType="com.newtouch.cloud.service.business.user.model.UserGroup">
    insert into user_group
    <trim prefix="(" suffix=")" suffixOverrides=",">
      <if test="userId != null">
        user_id,
      </if>
      <if test="groupId != null">
        group_id,
      </if>
    </trim>
    <trim prefix="values (" suffix=")" suffixOverrides=",">
      <if test="userId != null">
        #{userId,jdbcType=CHAR},
      </if>
      <if test="groupId != null">
        #{groupId,jdbcType=CHAR},
      </if>
    </trim>
  </insert>
</mapper>